{
  "name": "mycn",
  "version": "0.9.12",
  "description": "My Connector, a layer above relational database connectors",
  "main": "mycn.min.js",
  "types": "mycn.d.ts",
  "devDependencies": {
    "rollup": "0.56.5",
    "typescript": "2.7.2",
    "uglify-es": "3.3.9",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2"
